        xmlns:local="clr-namespace:SocketIOClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="MainText" Background="Black" Margin="23.225,10,46.954,90.144" Foreground="White" />
        <StackPanel Height="50" Margin="23.225,244.237,46.954,24.762" Orientation="Horizontal">
